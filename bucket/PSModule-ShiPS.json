{
    "version": "0.8.1",
    "homepage": "https://www.powershellgallery.com/packages/ShiPS",
    "hash": "e7cf0d5e3702d8250b3f3985dc0d764b7e9fa6e1dec4d86ddd51ee2735563537",
    "url": "https://raw.githubusercontent.com/sriramjiyer/MyScoopBucket/master/dummy.txt",
    "checkver": {
        "re": "([\\d.]+)\\s*\\(current version"
    },
    "autoupdate": {
        "url": "https://raw.githubusercontent.com/sriramjiyer/MyScoopBucket/master/dummy.txt"
    },
    "installer": {
        "script": "ensure $modulesdir;ensure_in_psmodulepath -dir $modulesdir -global $global;Save-Module -Name ShiPS -Path $modulesdir -RequiredVersion $version"
    },
    "uninstaller": {
        "script": "Remove-Item -Recurse -Force -Path \"$modulesdir/ShiPS/$version\";if ( -Not ( Get-ChildItem -Path \"$modulesdir/ShiPS\" ) ) { Remove-Item -Path \"$modulesdir/ShiPS\" }"
    }
}
